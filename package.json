    "frontend:dev": "cd frontend && npm run dev",
    "copy-artifacts": "cd backend && node scripts/copy-artifacts.js"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "ipfs",
    "transcript",
    "ledger"
  ],
